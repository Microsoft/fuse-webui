{
  "name": "@fuselab/ui-adal",
  "version": "1.5.0",
  "description": "azure login ui",
  "main": "lib/index.js",
  "bin": {
    "react-gen": "./bin/react-gen"
  },
  "scripts": {
    "test": "jest",
    "build": "webpack",
    "build-node": "tsc -p tsconfig.node.json & webpack --config webpack.node.js",
    "build-prod": "npm run build-node",
    "build-all": "webpack & npm run build-node",
    "tslint": "tslint -c tslint.json -t stylish --project tsconfig.json"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "moduleNameMapper": {
      "applicationinsights-js": "identity-obj-proxy"
    },
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest/preprocessor.js"
    },
    "testMatch": [
      "**/*.spec.(ts|tsx)"
    ],
    "collectCoverage": true,
    "coverageDirectory": "./coverage",
    "testURL": "http://localhost/"
  },
  "author": "hailiu@microsoft.com",
  "license": "MIT",
  "dependencies": {
    "@fuselab/ui-fabric": "^1.1.0",
    "@fuselab/ui-shared": "^1.1.0",
    "adal-angular": "^1.0.17",
    "react-redux": "~5.0.6",
    "react-router": "~4.2.0",
    "react-router-dom": "~4.2.2"
  },
  "devDependencies": {
    "@types/adal-angular": "^1.0.1",
    "@types/fetch-mock": "^6.0.3",
    "awesome-typescript-loader": "5.2.1",
    "fetch-mock": "^6.4.4",
    "html-webpack-plugin": "3.2.0",
    "jasmine": "^2.9.0",
    "jest": "^22.1.4",
    "merge": "1.2.1",
    "original": "1.0.2",
    "tslint": "5.12.1",
    "tslint-microsoft-contrib": "~5.0.2",
    "tslint-react": "~3.4.0",
    "typescript": "3.2.4",
    "uglify-js": "3.4.9",
    "uglifyjs-webpack-plugin": "2.1.1",
    "webpack": "4.29.0",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "3.1.14"
  },
  "peerDependencies": {
    "react": "16.8.3",
    "react-dom": "16.8.3",
    "redux-saga": "~0.16.0",
    "whatwg-fetch": "^2.0.3"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}
