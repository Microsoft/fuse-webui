{
  "name": "@fuselab/react-gen",
  "version": "1.6.1",
  "description": "code gen tool for react.js in typescript",
  "main": "./lib/cli.js",
  "bin": {
    "fuse-gen": "./lib/cli.js"
  },
  "scripts": {
    "test": "jest",
    "build": "tsc",
    "build-all": "tsc",
    "build-prod": "tsc",
    "tslint": "tslint -c tslint.json -t stylish --project tsconfig.json"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest/preprocessor.js"
    },
    "testMatch": [
      "**/*.spec.(ts|tsx)"
    ],
    "testPathIgnorePatterns": [
      "/examples/"
    ],
    "collectCoverage": true,
    "coverageDirectory": "./coverage",
    "testURL": "http://localhost/"
  },
  "dependencies": {
    "@fuselab/ui-shared": "^1.1.0",
    "chalk": "^2.4.2",
    "glob": "^7.1.3",
    "graceful-fs": "4.1.15",
    "inquirer": "^6.2.1",
    "rimraf": "^2.6.2",
    "underscore": "^1.8.3",
    "yargs": "^8.0.2"
  },
  "devDependencies": {
    "@types/inquirer": "^0.0.43",
    "@types/jasmine": "^2.8.6",
    "@types/node": "9.4.5",
    "@types/underscore": "^1.8.7",
    "awesome-typescript-loader": "5.2.1",
    "html-webpack-plugin": "3.2.0",
    "jasmine": "^2.9.0",
    "jest": "^22.1.4",
    "merge": "1.2.1",
    "original": "1.0.2",
    "tslint": "5.12.1",
    "tslint-microsoft-contrib": "~5.0.2",
    "tslint-react": "~3.4.0",
    "typescript": "3.2.4",
    "uglify-js": "3.4.9",
    "uglifyjs-webpack-plugin": "2.1.1",
    "webpack": "4.29.0",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "3.1.14"
  },
  "author": "hailiu@gmail.com",
  "license": "ISC",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}
